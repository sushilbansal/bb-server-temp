{
  "name": "bakbak-server",
  "version": "0.0.1",
  "scripts": {
    "start": "cross-env NODE_ENV=development ts-node-dev --ignore-watch node_modules --respawn src/index.ts",
    "build": "rimraf ./dist && tsc",
    "setup:testDB": "ts-node ./src/server/util/test/setupTestConn.ts",
    "test": "cross-env NODE_ENV=test npm run setup:testDB && jest",
